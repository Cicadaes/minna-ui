{
  "transform": {
    "^.+.js$": "babel-jest",
    "^.+\\.(html|svg)$": "@minna-ui/jest-config/lib/svelte-transform.js",
    "^.+\\.css$": "@minna-ui/jest-config/lib/null-transform.js"
  },
  "testPathIgnorePatterns": [
    "/\\.sapper/",
    "/app/manifest/",
    "/build/",
    "/coverage/",
    "/dist/",
    "/export/",
    "/node_modules/"
  ],
  "moduleFileExtensions": [
    "html",
    "js",
    "json",
    "mjs"
  ],
  "collectCoverageFrom": [
    "**/*.{html,js,mjs}",
    "!**/.*rc.js",
    "!**/*.config.js",
    "!**/.sapper/**",
    "!**/app/manifest/**",
    "!**/build/**",
    "!**/coverage/**",
    "!**/dist/**",
    "!**/export/**",
    "!**/fixtures/**",
    "!**/node_modules/**",
    "!**/test/**",
    "!**/webpack/**"
  ],
  "coverageDirectory": "<rootDir>/test/unit/coverage"
}
